local function createButton(text, callback, color)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 280, 0, 45)
    btn.BackgroundColor3 = color or Color3.fromRGB(60, 20, 80)
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 16
    btn.Text = text
    btn.Parent = ScrollFrame
  
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 8)
    btnCorner.Parent = btn
  
    local btnGradient = Instance.new("UIGradient")
    btnGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(80, 40, 100)),
        ColorSequenceKeypoint.new(1, color or Color3.fromRGB(60, 20, 80))
    }
    btnGradient.Rotation = 45
    btnGradient.Parent = btn
  
    local btnStroke = Instance.new("UIStroke")
    btnStroke.Color = Color3.fromRGB(138, 43, 226)
    btnStroke.Thickness = 1
    btnStroke.Transparency = 1
    btnStroke.Parent = btn
  
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(100, 60, 120)
        }):Play()
        TweenService:Create(btnStroke, TweenInfo.new(0.2), {
            Transparency = 0.3
        }):Play()
    end)
    
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {
            BackgroundColor3 = color or Color3.fromRGB(60, 20, 80)
        }):Play()
        TweenService:Create(btnStroke, TweenInfo.new(0.2), {
            Transparency = 1
        }):Play()
    end)
    
    btn.MouseButton1Click:Connect(callback)
    return btn
end


local function createTextBox(placeholder, callback)
    local box = Instance.new("TextBox")
    box.Size = UDim2.new(0, 280, 0, 45)
    box.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    box.TextColor3 = Color3.fromRGB(255, 255, 255)
    box.Font = Enum.Font.Gotham
    box.PlaceholderText = placeholder
    box.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    box.TextSize = 16
    box.ClearTextOnFocus = false
    box.Parent = ScrollFrame
    
    local boxCorner = Instance.new("UICorner")
    boxCorner.CornerRadius = UDim.new(0, 8)
    boxCorner.Parent = box
    
    local boxStroke = Instance.new("UIStroke")
    boxStroke.Color = Color3.fromRGB(80, 80, 80)
    boxStroke.Thickness = 2
    boxStroke.Parent = box
    
    box.Focused:Connect(function()
        TweenService:Create(boxStroke, TweenInfo.new(0.2), {
            Color = Color3.fromRGB(0, 150, 255)
        }):Play()
    end)
    
    box.FocusLost:Connect(function(enter)
        TweenService:Create(boxStroke, TweenInfo.new(0.2), {
            Color = Color3.fromRGB(80, 80, 80)
        }):Play()
        if enter then
            callback(box.Text)
        end
    end)
end


local StatusLabel = Instance.new("TextLabel")
StatusLabel.Size = UDim2.new(0, 280, 0, 25)
StatusLabel.BackgroundTransparency = 1
StatusLabel.Text = "Status: Ready"
StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
StatusLabel.Font = Enum.Font.Gotham
StatusLabel.TextSize = 14
StatusLabel.Parent = ScrollFrame


createTextBox(" Enter Speed (16-500)", function(value)
    local speed = tonumber(value)
    if speed and speed >= 16 and speed <= 500 then
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = speed
            StatusLabel.Text = "Status: Speed set to " .. speed
            StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
        end
    else
        StatusLabel.Text = "Status: Invalid speed value"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
    end
end)


local infinityJumpEnabled = false
local jumpBtn = createButton(" Toggle Infinity Jump: OFF", function()
    infinityJumpEnabled = not infinityJumpEnabled
    jumpBtn.Text = infinityJumpEnabled and " Toggle Infinity Jump: ON" or " Toggle Infinity Jump: OFF"
    jumpBtn.BackgroundColor3 = infinityJumpEnabled and Color3.fromRGB(0, 100, 0) or Color3.fromRGB(45, 45, 45)
    StatusLabel.Text = "Status: Infinity Jump " .. (infinityJumpEnabled and "Enabled" or "Disabled")
    StatusLabel.TextColor3 = infinityJumpEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 255, 0)
end, Color3.fromRGB(45, 45, 45))


UserInputService.JumpRequest:Connect(function()
    if infinityJumpEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)


createButton(" Bring Fuel", function()
    StatusLabel.Text = "Status: Collecting fuel..."
    StatusLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
    
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    local inventory = LocalPlayer:WaitForChild("Inventory")
    local sack


    local function findSack()
        for _, item in ipairs(inventory:GetChildren()) do
            if string.find(item.Name, "Sack") then
                return item
            end
        end
    end
    sack = findSack()
    if not sack then 
        StatusLabel.Text = "Status: No sack found!"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        return 
    end


    local function isSackFull()
        local current = sack:GetAttribute("NumberItems")
        local capacity = sack:GetAttribute("Capacity")
        return current and capacity and current >= capacity
    end


    local function store(item)
        local part = item:FindFirstChildWhichIsA("BasePart")
        if part then
            local lastPos = humanoidRootPart.CFrame
            humanoidRootPart.CFrame = part.CFrame
            task.wait(0.2)
            ReplicatedStorage.RemoteEvents.RequestBagStoreItem:InvokeServer(sack, item)
            task.wait(0.2)
            humanoidRootPart.CFrame = lastPos
        end
    end


    local collected = 0
    for _, item in ipairs(workspace.Items:GetChildren()) do
        if not isSackFull() and item.Name == "Coal" then
            store(item)
            collected = collected + 1
        end
    end
    
    StatusLabel.Text = "Status: Collected " .. collected .. " coal items"
    StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
end, Color3.fromRGB(100, 50, 0))


local killAuraEnabled = false
local auraBtn = createButton(" Toggle Kill Aura: OFF", function()
    killAuraEnabled = not killAuraEnabled
    auraBtn.Text = killAuraEnabled and " Toggle Kill Aura: ON" or " Toggle Kill Aura: OFF"
    auraBtn.BackgroundColor3 = killAuraEnabled and Color3.fromRGB(100, 0, 0) or Color3.fromRGB(45, 45, 45)
    StatusLabel.Text = "Status: Kill Aura " .. (killAuraEnabled and "Enabled" or "Disabled")
    StatusLabel.TextColor3 = killAuraEnabled and Color3.fromRGB(255, 0, 0) or Color3.fromRGB(255, 255, 0)
end, Color3.fromRGB(45, 45, 45))


createButton(" TP to Camp", function()
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")
    hrp.CFrame = CFrame.new(2, 4, -1)
    StatusLabel.Text = "Status: Teleported to camp"
    StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 255)
end, Color3.fromRGB(0, 50, 100))


local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local inventory = LocalPlayer:WaitForChild("Inventory")
local tool = inventory:WaitForChild("Old Axe")
local toolEvent = ReplicatedStorage:WaitForChild("RemoteEvents"):WaitForChild("ToolDamageObject")
local range = 20


RunService.Heartbeat:Connect(function()
    if not killAuraEnabled then return end


    for _, mob in pairs(workspace:WaitForChild("Characters"):GetChildren()) do
        local mobName = mob.Name
        if (mobName == "Wolf" or mobName == "Alpha Wolf" or mobName == "Polar Bear" or mobName == "Cultist") 
           and mob:FindFirstChild("HumanoidRootPart") then
           
            local distance = (humanoidRootPart.Position - mob.HumanoidRootPart.Position).Magnitude
            if distance <= range then
                local args = {
                    mob,
                    tool,
                    "30_4178961317",
                    mob.HumanoidRootPart.CFrame
                }
                pcall(function()
                    toolEvent:InvokeServer(unpack(args))
                end)
            end
        end
    end
end)


local function updateScrollSize()
    ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, Layout.AbsoluteContentSize.Y + 20)
end


Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateScrollSize)
updateScrollSize()


